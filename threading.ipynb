car3 = threading.Thread( target = Audi_A6_Allroud, name = '2.7 TDI', args = (5, '2.7 TDI') )
car3.start()

def Audi_A6_Allroud(s, engine):
    print( 'Ауді А6 з мотором {}. Повний привід Quattro, високий дорожній просвіт!'.format(engine) )
